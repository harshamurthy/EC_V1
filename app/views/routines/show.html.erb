<p id="notice"><%= notice %></p>
<h1>Routine number <%= @routine.id %></h1>
<p>
  <strong>Name:</strong>
  <%= @routine.description %>
</p>

<ul>
  <% @routine.exercises.each do |exercise| %>
    <li><%= exercise.name %></li>
  <% end %>
</ul>

<%= link_to 'Edit', edit_routine_path(@routine) %> |
<%= link_to 'Back', routines_path %>
